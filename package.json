{
  "name": "@regions-of-indonesia/svelte-query",
  "version": "2.5.1",
  "description": "Regions of Indonesia Svelte Query",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.modern.mjs",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "test-ui": "vitest --ui",
    "build": "microbundle src/index.ts --format modern,cjs --sourcemap false",
    "prerelease": "yarn run test && yarn run build",
    "release": "release-it --only-version"
  },
  "repository": "https://github.com/regions-of-indonesia/svelte-query.git",
  "author": "flamrdevs <flamrdevs@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "keywords": [
    "api",
    "library",
    "open-source",
    "javascript",
    "typescript",
    "indonesia",
    "region",
    "daerah",
    "wilayah",
    "wilayah-indonesia",
    "regions-of-indonesia",
    "svelte",
    "svelte-query"
  ],
  "dependencies": {
    "@regions-of-indonesia/client": "^3.0.0",
    "@tanstack/svelte-query": "^5.0.0-alpha.7",
    "svelte": "^3.57.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@testing-library/svelte": "^3.2.2",
    "@vitest/ui": "^0.29.7",
    "javascript-yesterday": "^1.2.0",
    "jsdom": "^21.1.1",
    "microbundle": "^0.15.1",
    "node-fetch": "^3.3.1",
    "release-it": "^15.9.0",
    "vitest": "^0.29.7"
  }
}
